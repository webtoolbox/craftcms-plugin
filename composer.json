{
    "name": "websitetoolbox/websitetoolboxcommunity",
    "description": "The Easiest CraftCMS Community Plugin. Effortlessly Build a Beautiful Community Forum with Zero Coding, Seamless Embedding, Instant Setup, and Exceptional Support.",
    "type": "craft-plugin",
    "version": "2.0.4",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "websitetoolboxcommunity"
    ],
    "support": {
        "docs": "https://github.com/webtoolbox/craftcms-plugin/blob/master/README.md",
        "issues": "https://github.com/webtoolbox/craftcms-plugin/blob/master/README.md"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Website Toolbox",
            "homepage": "http://websitetoolbox.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },    
    "require-dev": {
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "websitetoolbox\\community\\": "src/"
        }
    },
    "extra": {
        "hasCpSettings": true,
        "name": "Website Toolbox Community",
        "handle": "websitetoolboxforum",
        "developer": "Website Toolbox",
        "hasCpSection": false,
        "developerUrl": "http://websitetoolbox.com",
        "documentationUrl": "https://github.com/webtoolbox/craftcms-plugin/blob/master/README.md",
        "changelogUrl": "https://github.com/webtoolbox/craftcms-plugin/blob/master/README.md",
        "class": "websitetoolbox\\community\\Websitetoolboxcommunity",
        "components": {
            "sso": "websitetoolbox\\community\\services\\Sso"
        }
    }
  
}